#!/bin/bash -eu

source $OPENSHIFT_CARTRIDGE_SDK_BASH

case "$1" in
  -v|--version)
    version="$2"
esac

# Create additional directories required by the diy cartridge
mkdir -p ${OPENSHIFT_PYNADO_DIR}run

client_result "Disclaimer: On first push , this will automatically build Python and PIP Install:  Tornado , PyMongo , it will take around 10 mins+"

